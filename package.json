{
  "name": "flink-ecosystem",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@hapi/joi": "^15.1.0",
    "@reach/router": "^1.2.1",
    "@rooks/use-timeout": "^3.0.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "esm": "^3.2.25",
    "fetch-suspense": "^1.1.0",
    "get-form-data": "^2.0.0",
    "http-proxy-middleware": "^0.19.1",
    "js-cookie": "^2.2.0",
    "kcors": "^2.2.2",
    "knex": "^2.4.0",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-file-router": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.0",
    "querystring": "^0.2.0",
    "react": "^16.8.6",
    "react-animate-height": "^2.0.15",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.8.6",
    "react-dotdotdot": "^1.3.1",
    "react-dropzone": "^10.1.6",
    "react-ga": "^2.6.0",
    "react-markdown": "^4.1.0",
    "react-textarea-autosize": "^7.1.0",
    "react-use": "^10.3.0",
