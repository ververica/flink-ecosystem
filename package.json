    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/draft-js": "^0.10.34",
    "@types/js-cookie": "^2.2.2",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.8",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/reactstrap": "^8.0.1",
    "@types/styled-components": "^4.1.18",
    "concurrently": "^4.1.1",
    "husky": "^3.0.9",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "react-scripts": "3.0.1",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "build": "react-scripts build",
    "client": "react-scripts start",
    "flink-ecosystem-db": "docker run -d --rm -p 3306:3306 -v /tmp/flink-ecosystem-data:/var/lib/mysql -e MYSQL_ROOT_PASSWORD=test --name flink-ecosystem-db mariadb:10.3",
    "nodemon": "nodemon --signal SIGTERM -x 'npm run server-dev'",
    "prestart": "npm rebuild node-sass",
    "server-dev": "sleep 1 && NODE_PATH=src/ npm run server",
    "server": "node -r esm src/server/main.js",
    "start": "concurrently --kill-others-on-fail --prefix '[{name}]' --names 'client,server' 'npm run client' 'npm run nodemon'",
    "test": "react-scripts test --passWithNoTests"
  }
}
